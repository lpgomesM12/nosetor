<p id="notice"><%= notice %></p>

<h1>Listing St Itemprodutos</h1>

<table>
  <thead>
    <tr>
      <th>St item</th>
      <th>St produto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @st_itemprodutos.each do |st_itemproduto| %>
      <tr>
        <td><%= st_itemproduto.st_item %></td>
        <td><%= st_itemproduto.st_produto %></td>
        <td><%= link_to 'Show', st_itemproduto %></td>
        <td><%= link_to 'Edit', edit_st_itemproduto_path(st_itemproduto) %></td>
        <td><%= link_to 'Destroy', st_itemproduto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New St itemproduto', new_st_itemproduto_path %>
