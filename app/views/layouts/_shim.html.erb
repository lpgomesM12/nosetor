<div class="row" id="menuslin">
	<div class="col-lg-2">
	</div>
	<div class="col-lg-10">
		<div class="panel panel-default"  style="margin-top:60px">
			 <div class="panel-heading" style="background:#e89980" align="center"> <span aria-hidden="true"><b>Categorias</b></span>
			 </div>
		  <div class="panel-body">
             <% @categorias = StCategory.where(:flag_ativo => true).limit(15)%>
          <% @categorias.each do |categoria| %>
		     <a href="javascript:Pesquisar(<%=categoria.id%>);"><%=categoria.nome_categoria%></a><br>
	       <%end%>
            <br>
		     <button type="button"  class="btn btn-default" style="background:#e89980"  onclick="$('#myModal1').modal('show');" data-dismiss="modal">
             <span class="glyphicon glyphicon-plus-sign" aria-hidden="true"></span> Mais Categorias
		     </button>
		  </div>
		</div>
	</div>
</div>

